generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employee {
  id String @id @default(cuid())
  name String
  position String?
  department String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userId String
}
